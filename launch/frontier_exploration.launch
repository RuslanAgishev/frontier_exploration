<?xml version="1.0"?>

<launch>
	<arg name="bag_filename" default="$(find frontier_exploration)/launch/data.bag"/>
	<param name="/use_sim_time" value="true" />

	<!-- play bag file -->
	<node name="playbag" pkg="rosbag" type="play" args="--clock -r 3.33 $(arg bag_filename)" />

	<!-- planner -->
	<node pkg="frontier_exploration" type="planner.py" name="planner" output="screen"/>

	<!-- launch file include example -->
	<!-- <include file="$(find frontier_exploration)/launch/frontier_exploration.launch"/> -->

	<node name="rviz" pkg="rviz" type="rviz" required="true" args="-d $(find frontier_exploration)/config/config.rviz" />

</launch>
