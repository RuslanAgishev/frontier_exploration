<?xml version="1.0"?>

<launch>
	<arg name="bag_filename" default="$(find frontier_exploration)/launch/data.bag"/>
	<param name="/use_sim_time" value="true" />

	<!-- play bag file -->
	<node name="playbag" pkg="rosbag" type="play" args="--clock -r 1 $(arg bag_filename)" />

	<!-- modify realsense pointcloud -->
	<!-- <node pkg="vedu_segmentation" type="rs_vertical_segmask_to_pc.py" name="rs_segmask_to_pc" output="screen"/> -->

	<!-- move_base for costmap publishing -->
	<!-- <include file="$(find vedu_navigation)/launch/move_base.launch"/> -->

	<node name="rviz" pkg="rviz" type="rviz" required="true" args="-d $(find frontier_exploration)/config/config.rviz" />

</launch>
